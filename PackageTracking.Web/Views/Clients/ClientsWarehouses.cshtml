@model IEnumerable<PackageTracking.Data.Warehouse>
<p>
    @Html.ActionLink("Добавить", "Create", "Warehouses")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Client.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Client.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Details","Details","Warehouses", new { id = item.WarehouseId },null) |
                @Html.ActionLink("Edit", "Edit","Warehouses", new { id = item.WarehouseId },null) |
                @Html.ActionLink("Delete", "Delete","Warehouses", new { id = item.WarehouseId },null)
            </td>
        </tr>
    }

</table>


