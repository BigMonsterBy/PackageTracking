@model IEnumerable<PackageTracking.Data.Warehouse>
<p>
    @Html.ActionLink("Добавить", "Create", "Warehouses")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Client.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Client.Name)
            </td>
            <td>
                @Html.ActionLink(item.Name, "Edit", "Warehouses", new { id = item.WarehouseId }, null)
            </td>
            <td>
                @Html.ActionLink(item.Enabled ? "Выкл" : "Вкл", "Enable", "Warehouses", new { id = item.WarehouseId }, null)
            </td>
        </tr>
    }

</table>


